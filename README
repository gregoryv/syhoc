Publish new release

export TAG=v0.2.0
export PREV=v0.1.0

mkdir -p docs/$TAG
git restore --source $TAG -- spec.txt
mv spec.txt docs/$TAG

cd docs/$TAG
stp-v0.10.0 -i spec.txt -o index.html
cd .. # move into docs so the diff titles are nice
python3 pydiff.py -file-a $PREV/spec.txt -file-b $TAG/spec.txt -o $TAG/diff_$PREV.html

